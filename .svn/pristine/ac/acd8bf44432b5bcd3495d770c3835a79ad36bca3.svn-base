package com.netcracker.tc.shared.action;

import com.gwtplatform.dispatch.rpc.shared.UnsecuredActionImpl;


public class BeforeRegistrationMessageAction extends UnsecuredActionImpl<BeforeRegistrationMessageResult> {
    private String message;

    public BeforeRegistrationMessageAction() {
        message = "";
    }

    public BeforeRegistrationMessageAction(String message) {
        this.message = message;
    }

    public String getMessage(){
        return message;
    }

    public void setMessage(String message) {
        this.message = message;
    }
}
