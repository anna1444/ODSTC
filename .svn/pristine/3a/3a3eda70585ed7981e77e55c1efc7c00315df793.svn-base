package com.netcracker.tc.client.resource;

import com.google.gwt.resources.client.ClientBundle;
import com.google.gwt.resources.client.ImageResource;

/**
 * @author unconsionable
 */
public interface ImageBundle extends ClientBundle {

    static final String IMAGE_PATH = "com/netcracker/tc/image/";

    @Source(IMAGE_PATH + "logo_netcracker.png")
    ImageResource logoNetcracker();

    @Source(IMAGE_PATH + "main_header.jpg")
    ImageResource mainHeader();

    @Source(IMAGE_PATH + "user.resume.png")
    ImageResource userResume();

    @Source(IMAGE_PATH + "join_interview.png")
    ImageResource joinInterview();
}
