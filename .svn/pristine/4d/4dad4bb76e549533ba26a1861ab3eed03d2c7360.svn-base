package com.netcracker.tc.server.persistence.model.interview;

import com.netcracker.tc.server.persistence.model.user.User;

import java.io.Serializable;

/**
 * Created by unconsionable on 28.07.2014.
 */
public class InterviewSlot implements Serializable {

    private Long id;
    private User user;
    private Long time;

    private Interview interview;
    private InterviewResult interviewResult;

    public InterviewSlot() {
    }

    public Long getId() {
        return id;
    }

    public void setId(Long id) {
        this.id = id;
    }

    public User getUser() {
        return user;
    }

    public void setUser(User user) {
        this.user = user;
    }

    public Long getTime() {
        return time;
    }

    public void setTime(Long time) {
        this.time = time;
    }

    public Interview getInterview() {
        return interview;
    }

    public void setInterview(Interview interview) {
        this.interview = interview;
    }

    public InterviewResult getInterviewResult() {
        return interviewResult;
    }

    public void setInterviewResult(InterviewResult interviewResult) {
        this.interviewResult = interviewResult;
    }
}